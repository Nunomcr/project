package proPI;

import java.math.BigInteger;

public class ex48 {
	public static void main(String args[]) throws Exception{
		
		
		String contagem = ""; 
		BigInteger soma = BigInteger.ZERO;
		 
		for(int i = 1; i <= 1000; i++) {
		   soma = soma.add(BigInteger.valueOf(i).pow(i));	//faz a soma 1^1+...+1000^1000
		}
		
		contagem = soma.toString();							//converte em string
		contagem = contagem.substring(contagem.length()-10);//faz a contagem da string
		System.out.println(contagem);
			 
		}
	}


